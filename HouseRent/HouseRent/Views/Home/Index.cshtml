@inject HouseRent.Services.GetSettings service;
@{
    List<SettingsRH> settingsRHs = await service.GetSettingsAsnyc();
}
@model HomeViewModel
<!-- ======slider Area====== -->
<div class="slider-area">
    <div class="pogoSlider">
        @foreach (var item in Model.Sliders)
        {
            <div class="pogoSlider-slide" data-transition="expandReveal" data-duration="1000" style="background-image:url(uploads/sliders/@item.Img);">
                <div class="container-slider one">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="slider-text-content">
                                <h3 class="pogoSlider-slide-element" data-in="slideDown" data-out="slideUp" data-duration="500" data-delay="500">@item.Title1</h3>
                                <h2 class="pogoSlider-slide-element" data-in="slide-left" data-out="slideUp" data-duration="500" data-delay="500">@item.Title2</h2>
                                <p class="pogoSlider-slide-element" data-in="slideDown" data-out="slideUp" data-duration="500" data-delay="500">@item.Description</p>
                                <a href="@item.RedirectUrl" class="button pogoSlider-slide-element" data-in="slideDown" data-out="slideUp" data-duration="500" data-delay="500">Special Offer</a>
                            </div><!-- /.text-content -->
                        </div>

                        <!-- /.col-md-12 -->
                    </div><!-- /.row -->
                </div>

                <!-- /.container-slider -->
            </div>
        }

    </div><!-- .pogoSlider -->
</div><!-- /.slider-area container-fluid -->
<!-- ====== Category Menu ====== -->



<div class="category-menu margin-less-top">
    <div class="container-fluid pd-zero">
        <div class="row">
            <div class="col-md-12">
                <div class="category-menu-content">
                    <div class="category-title">
                        <h3><span>We Provide</span></h3>
                    </div><!-- /.category-titel -->
                    <div class="category-slider bg-white-smoke owl-carousel">
                        @foreach (var item in Model.ApartmentCategories)
                        {
                            <div class="item">
                                <div class="category-list style-one">
                                    <a href="#">
                                        <img src="~/uploads/categories/@item.ImgApart" alt="" />
                                        <h4>@item.AppartmentCategoryName</h4>
                                    </a>
                                </div><!-- /.category-list -->
                            </div>

                            <!-- /.item -->
                        }

                    </div><!-- /.category-slider -->
                </div><!-- /.category-menu-content -->
            </div> <!-- category-menu -->
        </div><!-- /.col-md-12 -->
    </div><!-- /.row -->
</div><!-- /.catagory-menu -->
