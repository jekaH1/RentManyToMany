<!-- ====== Fun Facts Area======= -->
<div class="fun-fects-area" style="background-image:url(assets/images/count-down-image.png)">
    <div class="container">
        <div class="stat">
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="milestone-counter">
                    <h3 class="stat-count highlight" data-form="1" data-to="@Model.appUsers.Count()" data-speed="3000">@Model.appUsers.Count()</h3>
                    <div class="milestone-details">Clients</div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="milestone-counter">
                    <h3 class="stat-count highlight" data-form="0" data-to="@Model.Orders.Count()" data-speed="3000">@Model.Orders.Count()</h3>
                    <div class="milestone-details">Booking</div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="milestone-counter">
                    <h3 class="stat-count highlight" data-form="1" data-to="@Model.Categories.Count()" data-speed="3000">@Model.Categories.Count()</h3>
                    <div class="milestone-details">Categories</div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="milestone-counter">
                    <h3 class="stat-count highlight" data-form="1" data-to="@Model.AllForCount.Count" data-speed="3000">@Model.AllForCount.Count</h3>
                    <div class="milestone-details">Apartment</div>
                </div>
            </div>
        </div><!-- stat -->
    </div><!-- /.container -->
</div><!-- /.fun-fects-area -->
